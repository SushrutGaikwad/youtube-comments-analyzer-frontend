{
    "manifest_version": 3,
    "name": "YouTube Sentiment Analysis",
    "version": "1.0",
    "description": "Analyze sentiments of YouTube comments.",
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": ["tabs"],
    "web_accessible_resources": [
        {
            "resources": ["secrets.json"],
            "matches": ["<all_urls>"]
        }
    ]
}